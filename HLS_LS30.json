{
  "description" : "Image collection format for the Harmonized Landsat Sentinel-2 (HLS-L30) dataset (v1.4)",
  "tags" : ["Landsat", "Sentinel", "Surface Reflectance"],
  "pattern" : ".+v1\\.4\\.hdf.*",
  "images" : {
    "pattern" : ".*(HLS\\.[LS]30.+)\\.v1\\.4\\.hdf.*"
  },
  "datetime" : {
    "pattern" : ".*HLS\\.[LS]30\\..{6}\\.([0-9]{7})\\.v1\\.4\\.hdf.*", 
    "format" : "%Y%j"
  },
  "bands" : {
    "Coastal_Aerosol" : {
      "nodata" : -1000,
      "pattern" : ".*B01$"
    },
    "Blue" : {
      "nodata" : -1000,
      "pattern" : ".*B02$"
    },
    "Green" : {
      "nodata" : -1000,
      "pattern" : ".*B03$"
    },
    "Red" : {
      "nodata" : -1000,
      "pattern" : ".*B04$"
    },
    "NIR" : {
      "nodata" : -1000,
      "pattern" : ".B8A$"
    },
    "SWIR1" : {
      "nodata" : -1000,
      "pattern" : ".*B11$"
    },
    "SWIR2" : {
      "nodata" : -1000,
      "pattern" : ".*B12$"
    },
    "QA" : {
      "pattern" : ".*QA$"
    }
  }
}
