{
  "description" : "Image collection format for HLS-L30 as downloaded from HLS. The format should work on original ZIP compressed as well as uncompressed imagery.",
  "tags" : ["Landasat","Sentinel","Surface Reflectance"],
  "pattern" : ".+\\.hdf",
  "images" : {
    "pattern" : "HDF4_EOS:EOS_GRID:(.+)"
  },
  "datetime" : {
    "pattern" : "HLS.[L-S]30.{7}.{7}",
    "format" : "%Y%j"
  },
  "bands" : {
    "B01" : {
      "nodata" : 0,
      "pattern" : "+band01$",
      "scale" : 0.0001
    },
    "B02" : {
      "nodata" : 0,
      "pattern" : "+band02$",
      "scale" : 0.0001
    },
    "B03" : {
      "nodata" : 0,
      "pattern" : "+band03$",
      "scale" : 0.0001
    },
    "B04" : {
      "nodata" : 0,
      "pattern" : "+band04$",
      "scale" : 0.0001
    },
    "B05" : {
      "nodata" : 0,
      "pattern" : "+band05$",
      "scale" : 0.0001
    },
    "B06" : {
      "nodata" : 0,
      "pattern" : "+band06$",
      "scale" : 0.0001
    },
    "B07" : {
      "nodata" : 0,
      "pattern" : "+band07$",
      "scale" : 0.0001
    },
    "B09" : {
      "nodata" : 0,
      "pattern" : "+band09$",
      "scale" : 0.0001
    },
    "B10" : {
      "nodata" : 0,
      "pattern" : "+band10$",
      "scale" : 0.01
    },
    "B11" : {
      "nodata" : 0,
      "pattern" : "+band11$",
      "scale" : 0.01
    },
    "QA" : {
      "pattern" : "+QA$"
    }
  }
}
